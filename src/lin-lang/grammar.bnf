τ ::=
    | int
    | (τ₁ ⊸ τ₂)
    | (τ₁ ⊗ ... ⊗  τₙ)
    | (ref τ)

binop := + | - | × | ÷

v ::=
    | x
    | n
    | (λ (x : τ₁) : τ₂ . e)
    | (v₁, ..., vₙ)
e ::=
    | v
    | (app v₁ v₂)
    | (let (x : τ) = e₁ in e₂)
    | (letprod (x₁ : τ₁) ... (xₙ : τₙ) = e₁ in e₂)
    | (if0 v then e₁ else e₂)
    | (v₁ binop v₂)
    | (new v)
    | (swap v₁ v₂)
    | (free v)

im := (import τ as x)
tl := ([export] let (x : τ) = e)

m := im* tl* [e]
